<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.example.base_project.base.BaseTextview
        android:id="@+id/baseTextview13"
        style="@style/txt_primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="@string/part_1"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.example.base_project.base.BaseTextview
        android:id="@+id/baseTextview14"
        style="@style/txt_primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="16dp"
        android:text="@string/part_2"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.example.base_project.base.BaseTextview
        android:id="@+id/txt_selected_part_1"
        style="@style/txt_primary"
        android:layout_marginBottom="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="3dp"
        android:text="00:00"
        android:textColor="#C9C3C3"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/baseTextview13"
        app:layout_constraintTop_toBottomOf="@+id/baseTextview13"
        tools:ignore="MissingConstraints" />

    <com.example.base_project.base.BaseTextview
        android:id="@+id/txt_selected_part_2"
        style="@style/txt_primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="3dp"
        android:text="00:00"
        android:textColor="#C9C3C3"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/baseTextview14"
        app:layout_constraintTop_toBottomOf="@+id/baseTextview14"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>